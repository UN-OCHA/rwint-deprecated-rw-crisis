<!DOCTYPE html>
<html lang="en" class="no-js">

<head>

  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  {{#each meta}}
    <meta name="{{name}}" content="{{content}}">
  {{/each}}

  <title>{{page-title}}</title>
  <link rel="apple-touch-icon" sizes="57x57" href="img/icons/output/icons/apple-touch-icon-57x57.png">
  <link rel="apple-touch-icon" sizes="60x60" href="img/icons/output/icons/apple-touch-icon-60x60.png">
  <link rel="apple-touch-icon" sizes="72x72" href="img/icons/output/icons/apple-touch-icon-72x72.png">
  <link rel="apple-touch-icon" sizes="76x76" href="img/icons/output/icons/apple-touch-icon-76x76.png">
  <link rel="apple-touch-icon" sizes="114x114" href="img/icons/output/icons/apple-touch-icon-114x114.png">
  <link rel="apple-touch-icon" sizes="120x120" href="img/icons/output/icons/apple-touch-icon-120x120.png">
  <link rel="apple-touch-icon" sizes="144x144" href="img/icons/output/icons/apple-touch-icon-144x144.png">
  <link rel="apple-touch-icon" sizes="152x152" href="img/icons/output/icons/apple-touch-icon-152x152.png">
  <link rel="apple-touch-icon" sizes="180x180" href="img/icons/output/icons/apple-touch-icon-180x180.png">
  <link rel="icon" type="image/png" href="img/icons/output/icons/favicon-32x32.png" sizes="32x32">
  <link rel="icon" type="image/png" href="img/icons/output/icons/android-chrome-192x192.png" sizes="192x192">
  <link rel="icon" type="image/png" href="img/icons/output/icons/favicon-96x96.png" sizes="96x96">
  <link rel="icon" type="image/png" href="img/icons/output/icons/favicon-16x16.png" sizes="16x16">
  <link rel="manifest" href="img/icons/output/icons/manifest.json">
  <link rel="shortcut icon" href="img/icons/output/icons/favicon.ico">
  <meta name="msapplication-TileColor" content="#da532c">
  <meta name="msapplication-TileImage" content="img/icons/output/icons/mstile-144x144.png">
  <meta name="msapplication-config" content="img/icons/output/icons/browserconfig.xml">
  <meta name="theme-color" content="#ffffff">

  <!-- build:css css/app.css -->
  <link href="css/styles.css" rel="stylesheet">
  <!-- endbuild -->

  <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
  <!--[if lt IE 9]>
  <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
  <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
  <![endif]-->

</head>

<body id="" data-spy=""data-target="">
<div class="crisis-page--header-nav">
  <header class="crisis-page--header">
    <h2 class="rw--name-logo">ReliefWeb</h2>
    <input type="checkbox" id="page-menu" name="page-menu" class="crisis-page--navigation--trigger"/>
    <label class="crisis-page--navigation--trigger-label" for="page-menu">
      <svg id="menu-button" width="40px" height="40px" viewBox="0 0 40 40" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:sketch="http://www.bohemiancoding.com/sketch/ns">
        <g id="Page-1" stroke="none" stroke-width="1" fill="none" fill-rule="evenodd" sketch:type="MSPage">
          <path d="M0,0 L12,0 L12,12 L0,12 L0,0 Z M0,14 L12,14 L12,26 L0,26 L0,14 Z M0,28 L12,28 L12,40 L0,40 L0,28 Z M14,0 L26,0 L26,12 L14,12 L14,0 Z M14,14 L26,14 L26,26 L14,26 L14,14 Z M14,28 L26,28 L26,40 L14,40 L14,28 Z M28,0 L40,0 L40,12 L28,12 L28,0 Z M28,14 L40,14 L40,26 L28,26 L28,14 Z M28,28 L40,28 L40,40 L28,40 L28,28 Z" id="brady-button" class="brady-button" fill="#665695" sketch:type="MSShapeGroup"></path>
        </g>
      </svg>
      <svg id="menu-button-close" width="40px" height="40px" viewBox="0 0 40 40" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:sketch="http://www.bohemiancoding.com/sketch/ns">
        <g id="Page-1" stroke="none" stroke-width="1" fill="none" fill-rule="evenodd" sketch:type="MSPage">
          <path d="M1.41420593,1.28785871e-14 L20.0022846,18.5880787 L20.0022846,18.5880787 L38.5903633,1.26565425e-14 L40,1.42108547e-14 L40,1.41879045 L21.4164982,20.0022923 L40,38.5857941 L40,40 L38.5857788,40 L20.0022846,21.4165058 L1.41879045,40 L0,40 L0,38.5903633 L18.5880711,20.0022923 L-4.4408921e-16,1.41422119 L0,1.42108547e-14 L1.41420593,1.42108547e-14 Z" id="Path" class="brady-button" fill="#000000" sketch:type="MSShapeGroup"></path>
        </g>
      </svg>
      <span class="text">Menu</span>
    </label>
    <nav class="crisis-page--navigation">
      <ul class="crisis-page--widget-nav">
        {{#each widgets}}
          <li><a class="crisis-page--widget-nav--item page-scroll" href="#{{slug}}">{{title}}</a></li>
        {{/each}}
        <li class="crisis-page--external-nav--item"><a href="http://reliefweb.int">ReliefWeb.int</a></li>
      </ul>
    </nav>
  </header>
  <h1 class="crisis-page--title">{{page-title}}</h1>
</div>

{{#each widgets}}
  <!-- {{title}} Widget -->
  <section id="{{slug}}" class="crisis-page--widget-container container content-section text-center"></section>
  <div class="clear-both"></div>
  <!-- end {{title}} Widget -->
{{/each}}

<div class="unsupported-browsers hidden">
  <p>This site is built to run on Internet Explorer 10 and above, as well as the latest versions of Apple Safari, Google Chrome, and Mozilla&nbsp;Firefox.</p>
  <p>Please update your browser or search for {{unsupported.searchTerms}} on ReliefWeb:</p>
  <div class="rw-buttons">
    {{#each unsupported.buttons}}
      <a class="btn" href="{{uri}}">{{text}}</a>
    {{/each}}
  </div>
</div>

<!-- build:js js/init.js -->
<script type="text/javascript" src="bower_components/jquery/dist/jquery.js"></script>
<script type="text/javascript" src="bower_components/jquery.browser/dist/jquery.browser.js"></script>
<script type="text/javascript" src="js/browser-detect.js"></script>
<!-- endbuild -->
<!-- build:js js/app.js -->
<script type="text/javascript" src="bower_components/modernizr/modernizr.js"></script>
<script type="text/javascript" src="bower_components/jquery-easing/jquery.easing.js"></script>
<script type="text/javascript" src="bower_components/oembed-client-cors/src/oembed-client-cors.js"></script>
<script type="text/javascript" src="bower_components/lodash/dist/lodash.js"></script>
<script type="text/javascript" src="bower_components/pym.js/dist/pym.js"></script>
<script type="text/javascript" src="js/iframe-builder.js"></script>
<script type="text/javascript" src="js/scrolling-nav.js"></script>
<!-- endbuild -->

<!-- Typekit -->
<script src="//use.typekit.net/hny7ecg.js"></script>
<script>try{Typekit.load();}catch(e){}</script>

{{#if environment.keys.google-analytics}}
<!-- Google Analytics -->
<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

  ga('create', '{{environment.keys.google-analytics}}', 'auto');
  ga('send', 'pageview');
</script>
{{/if}}
</body>

</html>
